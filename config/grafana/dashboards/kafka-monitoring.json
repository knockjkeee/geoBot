{
  "id": null,
  "uid": "kafka-microservices",
  "title": "Kafka Microservices Monitoring",
  "tags": [
    "kafka",
    "microservices",
    "monitoring"
  ],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": [
      "5s",
      "10s",
      "30s",
      "1m",
      "5m",
      "15m",
      "30m",
      "1h",
      "2h",
      "1d"
    ]
  },
  "panels": [
    {
      "id": 1,
      "type": "row",
      "title": "System Overview",
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "collapsed": false
    },
    {
      "id": 2,
      "type": "stat",
      "title": "Total Requests (Flask Producer)",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 0,
        "y": 1
      },
      "targets": [
        {
          "expr": "sum(rate(flask_http_request_total[5m]))",
          "legendFormat": "Requests/sec",
          "refId": "A"
        }
      ],
      "options": {
        "graphMode": "area",
        "colorMode": "value",
        "orientation": "auto",
        "textMode": "value_and_name",
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "green"
              },
              {
                "value": 50,
                "color": "yellow"
              },
              {
                "value": 100,
                "color": "red"
              }
            ]
          }
        }
      }
    },
    {
      "id": 3,
      "type": "stat",
      "title": "Messages Processed (User Processor)",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 6,
        "y": 1
      },
      "targets": [
        {
          "expr": "sum(rate(user_processor_messages_processed_total[5m]))",
          "legendFormat": "Messages/sec",
          "refId": "A"
        }
      ],
      "options": {
        "graphMode": "area",
        "colorMode": "value"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "color": {
            "mode": "palette-classic"
          }
        }
      }
    },
    {
      "id": 4,
      "type": "stat",
      "title": "Tasks Processed",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 12,
        "y": 1
      },
      "targets": [
        {
          "expr": "sum(rate(task_processor_tasks_processed_total[5m]))",
          "legendFormat": "Tasks/sec",
          "refId": "A"
        }
      ],
      "options": {
        "graphMode": "area",
        "colorMode": "value"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "color": {
            "mode": "palette-classic"
          }
        }
      }
    },
    {
      "id": 5,
      "type": "stat",
      "title": "Info Messages to ELK",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 18,
        "y": 1
      },
      "targets": [
        {
          "expr": "sum(rate(info_processor_messages_processed_total[5m]))",
          "legendFormat": "Messages/sec",
          "refId": "A"
        }
      ],
      "options": {
        "graphMode": "area",
        "colorMode": "value"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "color": {
            "mode": "palette-classic"
          }
        }
      }
    },
    {
      "id": 6,
      "type": "row",
      "title": "Request Performance",
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 5
      },
      "collapsed": false
    },
    {
      "id": 7,
      "type": "timeseries",
      "title": "Request Rate by Endpoint",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 6
      },
      "targets": [
        {
          "expr": "sum by (path) (rate(flask_http_request_total[5m]))",
          "legendFormat": "{{path}}",
          "refId": "A"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10
          },
          "unit": "reqps",
          "color": {
            "mode": "palette-classic"
          }
        }
      }
    },
    {
      "id": 8,
      "type": "timeseries",
      "title": "Response Time (p95, p99)",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 6
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(flask_http_request_duration_seconds_bucket[5m])))",
          "legendFormat": "p95",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.99, sum by (le) (rate(flask_http_request_duration_seconds_bucket[5m])))",
          "legendFormat": "p99",
          "refId": "B"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth"
          },
          "unit": "s",
          "color": {
            "mode": "palette-classic"
          }
        }
      }
    },
    {
      "id": 9,
      "type": "row",
      "title": "Processing Metrics",
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 14
      },
      "collapsed": false
    },
    {
      "id": 10,
      "type": "timeseries",
      "title": "User Processor - Processing Time",
      "gridPos": {
        "h": 6,
        "w": 8,
        "x": 0,
        "y": 15
      },
      "targets": [
        {
          "expr": "rate(user_processor_processing_seconds_sum[5m]) / rate(user_processor_processing_seconds_count[5m])",
          "legendFormat": "Avg processing time",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "color": {
            "mode": "continuous-GrYlRd"
          }
        }
      }
    },
    {
      "id": 11,
      "type": "timeseries",
      "title": "Task Processor - Processing Time",
      "gridPos": {
        "h": 6,
        "w": 8,
        "x": 8,
        "y": 15
      },
      "targets": [
        {
          "expr": "rate(task_processor_processing_seconds_sum[5m]) / rate(task_processor_processing_seconds_count[5m])",
          "legendFormat": "Avg processing time",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "color": {
            "mode": "continuous-GrYlRd"
          }
        }
      }
    },
    {
      "id": 12,
      "type": "timeseries",
      "title": "Info Processor - ELK Send Time",
      "gridPos": {
        "h": 6,
        "w": 8,
        "x": 16,
        "y": 15
      },
      "targets": [
        {
          "expr": "rate(info_processor_elk_send_seconds_sum[5m]) / rate(info_processor_elk_send_seconds_count[5m])",
          "legendFormat": "Avg ELK send time",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "color": {
            "mode": "continuous-GrYlRd"
          }
        }
      }
    },
    {
      "id": 13,
      "type": "row",
      "title": "Error Rates",
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 21
      },
      "collapsed": false
    },
    {
      "id": 14,
      "type": "timeseries",
      "title": "HTTP Error Rate (4xx, 5xx)",
      "gridPos": {
        "h": 6,
        "w": 12,
        "x": 0,
        "y": 22
      },
      "targets": [
        {
          "expr": "sum by (status) (rate(flask_http_request_total{status=~\"4..\"}[5m]))",
          "legendFormat": "4xx - {{status}}",
          "refId": "A"
        },
        {
          "expr": "sum by (status) (rate(flask_http_request_total{status=~\"5..\"}[5m]))",
          "legendFormat": "5xx - {{status}}",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "fillOpacity": 20
          },
          "unit": "reqps",
          "color": {
            "mode": "palette-classic"
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byRegexp",
              "options": "5xx"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "red",
                  "mode": "fixed"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byRegexp",
              "options": "4xx"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "yellow",
                  "mode": "fixed"
                }
              }
            ]
          }
        ]
      }
    },
    {
      "id": 15,
      "type": "timeseries",
      "title": "Processing Failures",
      "gridPos": {
        "h": 6,
        "w": 12,
        "x": 12,
        "y": 22
      },
      "targets": [
        {
          "expr": "rate(task_processor_tasks_failed_total[5m])",
          "legendFormat": "Task failures",
          "refId": "A"
        },
        {
          "expr": "rate(info_processor_messages_failed_total[5m])",
          "legendFormat": "Info failures",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "color": {
            "mode": "palette-classic"
          }
        }
      }
    },
    {
      "id": 16,
      "type": "row",
      "title": "Active Connections & Tasks",
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 28
      },
      "collapsed": false
    },
    {
      "id": 17,
      "type": "gauge",
      "title": "Active Tasks",
      "gridPos": {
        "h": 6,
        "w": 8,
        "x": 0,
        "y": 29
      },
      "targets": [
        {
          "expr": "task_processor_active_tasks",
          "legendFormat": "Active tasks",
          "refId": "A"
        }
      ],
      "options": {
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "green"
              },
              {
                "value": 50,
                "color": "yellow"
              },
              {
                "value": 80,
                "color": "red"
              }
            ]
          }
        }
      }
    },
    {
      "id": 18,
      "type": "stat",
      "title": "Active Containers",
      "gridPos": {
        "h": 6,
        "w": 16,
        "x": 8,
        "y": 29
      },
      "targets": [
        {
          "expr": "sum by (service) (orchestrator_active_containers)",
          "legendFormat": "{{service}}",
          "refId": "A"
        }
      ],
      "options": {
        "graphMode": "none",
        "colorMode": "value",
        "textMode": "value_and_name"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "palette-classic"
          }
        }
      }
    }
  ],
  "templating": {
    "list": [
      {
        "name": "interval",
        "type": "interval",
        "options": [
          {
            "text": "1m",
            "value": "1m"
          },
          {
            "text": "5m",
            "value": "5m"
          },
          {
            "text": "10m",
            "value": "10m"
          },
          {
            "text": "30m",
            "value": "30m"
          },
          {
            "text": "1h",
            "value": "1h"
          }
        ],
        "current": {
          "text": "5m",
          "value": "5m"
        }
      }
    ]
  },
  "annotations": {
    "list": [
      {
        "name": "Deployments",
        "datasource": "Prometheus",
        "enable": true,
        "expr": "changes(up[5m]) > 0",
        "iconColor": "blue",
        "tagKeys": "service"
      }
    ]
  },
  "overwrite": true
}